{
  "name": "covid-risk-stratification-client",
  "version": "1.2",
  "description": "COVID-19 Clinical Risk Assessment Tool Client",
  "main": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/DataS-DHSC/nhsd-risk-strat-clinical-tool-dev"
  },
  "scripts": {
    "prepare": "[ -d .git ] && husky install || true",
    "install:chromedriver": "./src/build/install-chromedriver.sh",
    "licenses:update": "yarn run license-checker --production --relativeLicensePath ----customPath src/resources/open-source/license-checker-customformat.json  --json --out src/resources/open-source/licenses-npm.json",
    "licenses:check": "yarn run license-checker --production --relativeLicensePath ----customPath src/resources/open-source/license-checker-customformat.json  --json --out .build/licenses-npm-check.json && cmp src/resources/open-source/licenses-npm.json .build/licenses-npm-check.json || (echo ; echo ERROR: License file not up to date.  Run yarn licenses:update ; echo ; exit 1) ",
    "licenses:summary": "yarn run license-checker --production --summary",
    "test:unit": "jest -c src/__test__/config/jest.unit.config.js",
    "test:unit:coverage": "yarn run test:unit --coverage",
    "test:unit:watch": "yarn run test:unit --watch",
    "test:unit:update-snapshots": "yarn run test:unit --updateSnapshot",
    "test:allure:serve": "allure serve reports/allure-results --host localhost",
    "test:allure:generate": "allure generate  reports/allure-results --clean -o reports/html/${TEST_TYPE} || true",
    "fmt": "prettier --write .",
    "lint": "prettier --check . && eslint --ext .js src/ test/integration/ .*.js",
    "lint:watch": "yarn run lint -- --watch",
    "lint:fix": "yarn run lint --fix",
    "lint:style": "yarn stylelint src/styles/*.scss",
    "dev": "next -p 3010",
    "build": "next build",
    "start": "next start -p 3010",
    "build-start": "yarn run build && yarn run start",
    "audit": "auditjs ossi --xml | tee audit.xml",
    "security-scan": "hawkeye scan"
  },
  "author": "",
  "license": "ISC",
